  "description": "Node.JS Accounting Ledger System",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules ui/lib",
    "docker:build": "docker build -t reekoheek/node-account .",
    "docker:shell": "docker run --rm -ti reekoheek/node-account sh",
    "docker:up": "docker run -d --name node-account -p 3000:3000 reekoheek/node-account",
    "docker:down": "docker stop node-account",
    "dev": "webpack-dev-server",
    "dashboard": "webpack-dashboard -- webpack-dev-server --env.dashboard=1",
    "build": "webpack",
    "start": "nodemon -w . -w ~/Workspaces/js/node-norm --harmony-async-await server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
